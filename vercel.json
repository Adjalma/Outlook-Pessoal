{
  "version": 2,
  "buildCommand": "npm install && npm run build",
  "installCommand": "npm install --legacy-peer-deps",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node@3.0.7"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "server.js"
    },
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ]
}